{% block modal %}
<div class="modal fade btn-media-modal" tabindex="-1" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">{{ 'btn_media.media.choose'|trans }}</h4>
            </div>
            {% include 'BtnMediaBundle:MediaModal:_body.html.twig' %}
        </div>
    </div>
    {% include 'BtnMediaBundle:MediaModal:_partials.html.twig' %}
</div>
{% endblock %}
